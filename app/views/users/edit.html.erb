<%= form_with model: @user do |f| %>
 <div>
  <p >Name</p>
  <%= f.text_field :title %>
 </div>
 <div>
   <p>Image</p>
   <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>
 </div>
 <div>
  <p>Introduction</p>
  <%= f.text_area :body %>
 </div>
  <%= f.submit "Update User", class: "btn btn-secondary btn-sm" %>
 <% end %>